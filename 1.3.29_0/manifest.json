{
   "action": {
      "default_icon": {
         "48": "assets/images/48.png"
      },
      "default_popup": "popup/index.html",
      "default_title": "Etsy Helper"
   },
   "background": {
      "service_worker": "scripts/background.js"
   },
   "content_scripts": [ {
      "css": [ "assets/styles/jquery.toast.css", "assets/styles/content.css" ],
      "js": [ "assets/libs/jquery.min.js", "assets/libs/jquery.toast.js", "scripts/content_script.js", "scripts/sync_order.js", "scripts/add_tracking.js", "scripts/settings.js", "scripts/etsy-marketing/change-style.js" ],
      "matches": [ "https://www.etsy.com/*", "https://www.yuntrack.com/*" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'",
      "sandbox": "sandbox allow-scripts; script-src 'self' 'https://www.etsy.com/*'; object-src 'self'"
   },
   "default_locale": "en",
   "description": "An extension to help you fulfill Etsy orders easier with MerchBridge.",
   "devtools_page": "devtool/devtool.html",
   "host_permissions": [ "https://www.etsy.com/*", "https://api-dev.merchbridge.com/*", "https://api.merchbridge.com/*", "http://127.0.0.1:8080/*", "https://www.yuntrack.com/*" ],
   "icons": {
      "128": "assets/images/128.png",
      "16": "assets/images/16.png",
      "48": "assets/images/48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "MerchBridge Etsy Helper",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3.29",
   "web_accessible_resources": [ {
      "matches": [ "https://www.etsy.com/*", "https://www.yuntrack.com/*" ],
      "resources": [ "assets/images/not-found.png", "assets/images/completed.png", "assets/styles/etsy-marketing/style.css", "injected.js" ]
   } ]
}
